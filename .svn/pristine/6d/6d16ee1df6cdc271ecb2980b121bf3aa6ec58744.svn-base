banner.charset=UTF-8
server.tomcat.uri-encoding=UTF-8
spring.http.encoding.charset=UTF-8
spring.http.encoding.enabled=true
spring.http.encoding.force=true
spring.message.encoding=UTF-8
##################
##   mybatis配置   ##
##################
mybatis.type-aliases-package=priv.zjay.all.entity
mybatis.table.auto=update
mybatis.model.pack=priv.zjay.all.entity
mybatis.database.type=mysql
##################
##   数据源配置   ##
##################
spring.datasource.driver-class-name = com.mysql.jdbc.Driver
spring.datasource.url = jdbc:mysql://127.0.0.1:3306/test?useUnicode=true&characterEncoding=utf-8&useSSL=false
spring.datasource.username = root
spring.datasource.password = root

#Server
server.port=8080

security.basic.enabled=false
management.security.enabled=false

##########################
##   页面用freemarker   ##
#########################
spring.freemarker.request-context-attribute=req
spring.freemarker.suffix=.ftl
spring.freemarker.cache=false
spring.freemarker.template-loader-path=classpath:/templates/
spring.freemarker.content-type=text/html
spring.freemarker.charset=UTF-8
spring.freemarker.enabled=true
#spring.freemarker.settings.number_format='0.##'
      
#设置上传文件大小 -1代表不限制
spring.http.multipart.maxFileSize=-1
spring.http.multipart.maxRequestSize=-1

#后台自动打印执行的sql
logging.level.com.telecom.mapper=debug

# tomcat最大线程数，默认为200  
server.tomcat.max-threads=800
# tomcat的URI编码  
server.tomcat.uri-encoding=UTF-8
